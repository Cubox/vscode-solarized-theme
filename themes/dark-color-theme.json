    {
      "name": "* in import * as",
      "scope": "constant.language.import-export-all.js",
      "settings": {
        "foreground": "#839496" // fg
      }
    },

    //  Typedef    A typedef

    //  SpecialChar    special character in a constant
    //  Debug      debugging statements

    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#93a1a1" // base1
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#93a1a1" // base1
      }
    },
    {
      "name": "markup bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "markup italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "markup underline",
      "scope": "meta.link.inline.description.markdown",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "markup bold/italic",
      "scope": "markup.italic markup.bold | markup.bold markup.italic",
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "markup bold/underline",
